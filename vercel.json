{
    "buildCommand": "npx vite build",
    "outputDirectory": "dist",
    "devCommand": "npx vite dev",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/about",
            "destination": "/about.html"
        },
        {

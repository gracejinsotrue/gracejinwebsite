            "destination": "/projects/project1/index.html"
        },
        {
            "source": "/projects/handsome-programming-languages",
            "destination": "/projects/handsome-programming-languages/index.html"
        },
        {
            "source": "/projects/game-thing",
            "destination": "/projects/game-thing/index.html"
